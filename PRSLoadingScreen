    "rbxassetid://3319895658", --["White Cat"] // rarity = "Common"
    "rbxassetid://3321273354", --["Wolf"] // rarity = "Common"
    "rbxassetid://3321273024", --["Shiny Bat"] // rarity = "Common"
    "rbxassetid://3321301027", --["Shiny Blue Butterfly"] // rarity = "Common"
    "rbxassetid://3321301083", --["Shiny Brown Fox"] // rarity = "Common"
    "rbxassetid://3319957475", --["Shiny Dog"] // rarity = "Common"
    "rbxassetid://3319957578", --["Shiny Fox"] // rarity = "Common"
    "rbxassetid://3321273075", --["Shiny Ice Cat"] // rarity = "Common"
    "rbxassetid://3321396991", --["Shiny Polar Bear"] // rarity = "Common"
    "rbxassetid://3710867266", --["Shiny Red Butterfly"] // rarity = "Common"
    "rbxassetid://3321328346", --["Shiny Snow Fox"] // rarity = "Common"
    "rbxassetid://3324883683", --["Shiny Sweets Bat"] // rarity = "Common"
    "rbxassetid://3319957680", --["Shiny White Cat"] // rarity = "Common"
    "rbxassetid://3321273272" --["Shiny Wolf"] // rarity = "Common"
    }
    
    local petContainer = Instance.new("Frame")
    petContainer.Name = "PetContainer"
    petContainer.Size = UDim2.fromScale(1, 1)
    petContainer.BackgroundTransparency = 1
    petContainer.Parent = mainContainer
    
    for i = 1, 8 do
        local petImage = petImages[math.random(1, #petImages)]
        local pet = Instance.new("ImageLabel")
        pet.Size = UDim2.fromOffset(math.random(80, 120), math.random(80, 120))
        pet.Position = UDim2.fromScale(math.random(), math.random())
        pet.BackgroundTransparency = 1
        pet.Image = petImage
        pet.ImageTransparency = 0.2
        pet.ZIndex = 5
        pet.Parent = petContainer
        
        spawn(function()
            local startX = pet.Position.X.Scale
            local startY = pet.Position.Y.Scale
            local offsetX = math.random(-10, 10) / 100
            local offsetY = math.random(-10, 10) / 100
            local rotationSpeed = math.random(-5, 5)
            local currentRotation = 0
            
            while pet.Parent and isLoadingScreenActive do
                local moveTime = math.random(3, 6)
                local targetX = math.clamp(startX + offsetX, 0.1, 0.9)
                local targetY = math.clamp(startY + offsetY, 0.1, 0.9)
                
                local moveTween = TweenService:Create(
                    pet,
                    TweenInfo.new(moveTime, Enum.EasingStyle.Sine, Enum.EasingDirection.InOut),
                    {Position = UDim2.fromScale(targetX, targetY)}
                )
                moveTween:Play()
                
                -- Rotate the pet while it moves
                for i = 1, moveTime * 10 do
                    if not pet.Parent or not isLoadingScreenActive then break end
                    currentRotation = currentRotation + rotationSpeed / 10
                    pet.Rotation = math.sin(currentRotation) * 15
                    wait(0.1)
                end
                
                -- Reverse direction
                offsetX = -offsetX + math.random(-5, 5) / 100
                offsetY = -offsetY + math.random(-5, 5) / 100
                startX = targetX
                startY = targetY
            end
        end)
    end
end

-- Preload assets
local assetsToPreload = {
    "rbxassetid://6444378561", -- Paw print/star image
    "rbxassetid://103945760063122", -- Game icon
}

-- Add pet images to preload
for _, petImage in ipairs(petImages or {}) do
    table.insert(assetsToPreload, petImage)
end

local function preloadAssets()
    local assets = {}
    for _, assetId in ipairs(assetsToPreload) do
        table.insert(assets, assetId)
    end
    
    ContentProvider:PreloadAsync(assets, function(assetId, status)
        if status == Enum.AssetFetchStatus.Failure then
            warn("Failed to preload asset: " .. assetId)
        end
    end)
end

-- Start the loading sequence
saveAndHideGuis()
setupCamera()
preloadAssets()
simulateLoading()
createFloatingPets()
