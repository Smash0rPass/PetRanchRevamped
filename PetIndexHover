tokenValueText.TextSize = 16
tokenValueText.TextXAlignment = Enum.TextXAlignment.Left
tokenValueText.Text = "Rebirth Tokens: +0"
tokenValueText.ZIndex = 11
tokenValueText.Visible = false
tokenValueText.LayoutOrder = 3
tokenValueText.Parent = statsContainer

local tokenStroke = Instance.new("UIStroke")
tokenStroke.Thickness = 2
tokenStroke.Color = Color3.fromRGB(150, 50, 255)
tokenStroke.Parent = tokenValueText

local hasShinyText = Instance.new("TextLabel")
hasShinyText.Name = "HasShinyVersion"
hasShinyText.Size = UDim2.new(1, 0, 0, 25)
hasShinyText.BackgroundTransparency = 1
hasShinyText.Font = Enum.Font.FredokaOne
hasShinyText.TextColor3 = Color3.fromRGB(255, 255, 255)
hasShinyText.TextSize = 16
hasShinyText.TextXAlignment = Enum.TextXAlignment.Left
hasShinyText.Text = "Has Shiny: None"
hasShinyText.ZIndex = 11
hasShinyText.LayoutOrder = 4
hasShinyText.Parent = statsContainer

local hasShinyStroke = Instance.new("UIStroke")
hasShinyStroke.Thickness = 2
hasShinyStroke.Color = Color3.fromRGB(46, 46, 53)
hasShinyStroke.Parent = hasShinyText

local isShinyText = Instance.new("TextLabel")
isShinyText.Name = "IsShiny"
isShinyText.Size = UDim2.new(1, 0, 0, 25)
isShinyText.BackgroundTransparency = 1
isShinyText.Font = Enum.Font.FredokaOne
isShinyText.TextColor3 = Color3.fromRGB(255, 255, 255)
isShinyText.TextSize = 16
isShinyText.TextXAlignment = Enum.TextXAlignment.Left
isShinyText.Text = "Is Shiny: False"
isShinyText.ZIndex = 11
isShinyText.LayoutOrder = 5
isShinyText.Parent = statsContainer

local isShinyStroke = Instance.new("UIStroke")
isShinyStroke.Thickness = 2
isShinyStroke.Color = Color3.fromRGB(255, 0, 0)
isShinyStroke.Parent = isShinyText

local paddingFrame = Instance.new("Frame")
paddingFrame.Name = "BottomPadding"
paddingFrame.Size = UDim2.new(1, 0, 0, 10)
paddingFrame.BackgroundTransparency = 1
paddingFrame.LayoutOrder = 6
paddingFrame.Parent = statsContainer

local colorCycleConnections = {}

local function formatNumber(number)
    local formatted = tostring(number)
    local k
    while true do
        formatted, k = string.gsub(formatted, "^(-?%d+)(%d%d%d)", '%1,%2')
        if k == 0 then break end
    end
    return formatted
end

local function applyLegendaryCycle(stroke)
    if colorCycleConnections[stroke] then
        colorCycleConnections[stroke]:Disconnect()
        colorCycleConnections[stroke] = nil
    end
    
    local r, g, b = 255, 0, 0
    local phase = 0
    
    local connection = game:GetService("RunService").Heartbeat:Connect(function()
        if not stroke or not stroke.Parent then
            if colorCycleConnections[stroke] then
                colorCycleConnections[stroke]:Disconnect()
                colorCycleConnections[stroke] = nil
            end
            return
        end
        
        local step = 1
        
        if phase == 0 then
            g = g + step
            if g >= 255 then
                g = 255
                phase = 1
            end
        elseif phase == 1 then
            r = r - step
            if r <= 0 then
                r = 0
                phase = 2
            end
        elseif phase == 2 then
            b = b + step
            if b >= 255 then
                b = 255
                phase = 3
            end
        elseif phase == 3 then
            g = g - step
            if g <= 0 then
                g = 0
                phase = 4
            end
        elseif phase == 4 then
            r = r + step
            if r >= 255 then
                r = 255
                phase = 5
            end
        elseif phase == 5 then
            b = b - step
            if b <= 0 then
                b = 0
                phase = 0
            end
        end
        
        r = math.clamp(r, 0, 255)
        g = math.clamp(g, 0, 255)
        b = math.clamp(b, 0, 255)
        
        stroke.Color = Color3.fromRGB(r, g, b)
    end)
    
    colorCycleConnections[stroke] = connection
end

local function applyMythicCycle(stroke)
    if colorCycleConnections[stroke] then
        colorCycleConnections[stroke]:Disconnect()
        colorCycleConnections[stroke] = nil
    end
    
    local time = 0
    
    local connection = game:GetService("RunService").Heartbeat:Connect(function(deltaTime)
        if not stroke or not stroke.Parent then
            if colorCycleConnections[stroke] then
                colorCycleConnections[stroke]:Disconnect()
                colorCycleConnections[stroke] = nil
            end
            return
        end
        
        time = time + deltaTime * 3
        
        local cycleValue = (math.sin(time) + 1) / 2
        
        local darkRedR, darkRedG, darkRedB = 120, 24, 24
        local lightRedR, lightRedG, lightRedB = 255, 51, 51
        
        local r = math.floor(darkRedR + (lightRedR - darkRedR) * cycleValue)
        local g = math.floor(darkRedG + (lightRedG - darkRedG) * cycleValue)
        local b = math.floor(darkRedB + (lightRedB - darkRedB) * cycleValue)
        
        stroke.Color = Color3.fromRGB(r, g, b)
    end)
    
    colorCycleConnections[stroke] = connection
end

local function applyDivineCycle(stroke)
    if colorCycleConnections[stroke] then
        colorCycleConnections[stroke]:Disconnect()
        colorCycleConnections[stroke] = nil
    end
    
    local time = 0
    
    local connection = game:GetService("RunService").Heartbeat:Connect(function(deltaTime)
        if not stroke or not stroke.Parent then
            if colorCycleConnections[stroke] then
                colorCycleConnections[stroke]:Disconnect()
                colorCycleConnections[stroke] = nil
            end
            return
        end
        
        time = time + deltaTime * 2
        
        local cycleValue = (math.sin(time) + 1) / 2
        
        local purpleR, purpleG, purpleB = 128, 0, 255
        local pinkR, pinkG, pinkB = 255, 100, 255
        
        local r = math.floor(purpleR + (pinkR - purpleR) * cycleValue)
        local g = math.floor(purpleG + (pinkG - purpleG) * cycleValue)
        local b = math.floor(purpleB + (pinkB - purpleB) * cycleValue)
        
        stroke.Color = Color3.fromRGB(r, g, b)
    end)
    
    colorCycleConnections[stroke] = connection
end

local function applyPremiumCycle(stroke)
    if colorCycleConnections[stroke] then
        colorCycleConnections[stroke]:Disconnect()
        colorCycleConnections[stroke] = nil
    end
    
    local time = 0
    
    local connection = game:GetService("RunService").Heartbeat:Connect(function(deltaTime)
        if not stroke or not stroke.Parent then
            if colorCycleConnections[stroke] then
                colorCycleConnections[stroke]:Disconnect()
                colorCycleConnections[stroke] = nil
            end
            return
        end
        
        time = time + deltaTime * 2
        
        local cycleValue = (math.sin(time) + 1) / 2
        
        local darkGoldR, darkGoldG, darkGoldB = 200, 180, 40
        local lightGoldR, lightGoldG, lightGoldB = 255, 204, 51
        
        local r = math.floor(darkGoldR + (lightGoldR - darkGoldR) * cycleValue)
        local g = math.floor(darkGoldG + (lightGoldG - darkGoldG) * cycleValue)
        local b = math.floor(darkGoldB + (lightGoldB - darkGoldB) * cycleValue)
        
        stroke.Color = Color3.fromRGB(r, g, b)
    end)
    
    colorCycleConnections[stroke] = connection
end

local function setRarityColor(rarityStroke, rarity)
    if colorCycleConnections[rarityStroke] then
        colorCycleConnections[rarityStroke]:Disconnect()
        colorCycleConnections[rarityStroke] = nil
    end
    if rarity == "Common" then
        rarityStroke.Color = Color3.fromRGB(150, 150, 150)
    elseif rarity == "Uncommon" then
        rarityStroke.Color = Color3.fromRGB(126, 255, 173)
    elseif rarity == "Rare" then
        rarityStroke.Color = Color3.fromRGB(5, 138, 255)
    elseif rarity == "Epic" then
        rarityStroke.Color = Color3.fromRGB(255, 0, 201)
    elseif rarity == "Legendary" then
        applyLegendaryCycle(rarityStroke)
    elseif rarity == "Mythic" then
        applyMythicCycle(rarityStroke)
    elseif rarity == "Divine" then
        applyDivineCycle(rarityStroke)
    elseif rarity == "Premium" then
        applyPremiumCycle(rarityStroke)
    end
end

local function applyShinyEffect(isShiny)
    if colorCycleConnections[isShinyStroke] then
        colorCycleConnections[isShinyStroke]:Disconnect()
        colorCycleConnections[isShinyStroke] = nil
    end
    
    if isShiny then
        isShinyText.Text = "Is Shiny: True"
        isShinyStroke.Color = Color3.fromRGB(0, 255, 0)
    else
        isShinyText.Text = "Is Shiny: False"
        isShinyStroke.Color = Color3.fromRGB(255, 0, 0)
    end
end

local function updateHoverFrame(petName, info, petImageInstance)
    titleText.Text = petName
    rarityText.Text = "Rarity: " .. info.rarity
    coinValueText.Text = "Coin/Seconds: " .. formatNumber(info.coinValue)
    
    if info.tokenValue then
        tokenValueText.Text = "Rebirth Tokens: +" .. formatNumber(info.tokenValue)
        tokenValueText.Visible = true
    else
        tokenValueText.Visible = false
    end
    
    if info.Shiny then
        hasShinyText.Text = "Has Shiny: " .. info.Shiny
    else
        hasShinyText.Text = "Has Shiny: None"
    end
    
    if petName:match("^Shiny") then
        applyShinyEffect(true)
    else
        applyShinyEffect(false)
    end
    
    setRarityColor(rarityStroke, info.rarity)
    
    -- Update pet image display
    petImageDisplay.Image = petImageInstance.Image
    petImageDisplay.ImageColor3 = petImageInstance.ImageColor3
    petImageDisplay.ImageTransparency = petImageInstance.ImageTransparency
    petImageDisplay.ScaleType = petImageInstance.ScaleType or Enum.ScaleType.Fit
end

local UserInputService = game:GetService("UserInputService")
local function updateFramePosition()
    local mousePos = UserInputService:GetMouseLocation()
    hoverFrame.Position = UDim2.new(0, mousePos.X + 15, 0, mousePos.Y - 40)
end

UserInputService.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement then
        updateFramePosition()
    end
end)

local function connectToPetImages()
    local petIndexPath = game:GetService("Players").LocalPlayer.PlayerGui.MainGui.CenterUIFrame.PetIndex.PetIndexInnerFrame.PetIndexScrollingFrame
    
    if not petIndexPath then
        repeat
            wait(0.5)
            petIndexPath = game:GetService("Players").LocalPlayer.PlayerGui.MainGui.CenterUIFrame.PetIndex.PetIndexInnerFrame.PetIndexScrollingFrame
        until petIndexPath or wait(10)
    end
    
    if not petIndexPath then
        return
    end
    
    local function connectToPetButton(button)
        if button:IsA("ImageButton") and button:FindFirstChild("PetImage") then
            local petImage = button.PetImage
            
            petImage.MouseEnter:Connect(function()
                local imageId = petImage.Image
                local petName, info = getPetNameFromImageId(imageId)
                
                if petName and info then
                    updateHoverFrame(petName, info, petImage)
                    updateFramePosition()
                    hoverFrame.Visible = true
                end
            end)
            
            petImage.MouseLeave:Connect(function()
                hoverFrame.Visible = false
            end)
        end
    end
    
    for _, child in pairs(petIndexPath:GetDescendants()) do
        connectToPetButton(child)
    end
    
    petIndexPath.DescendantAdded:Connect(connectToPetButton)
end

local function cleanupConnections()
    for stroke, connection in pairs(colorCycleConnections) do
        if connection then
            connection:Disconnect()
        end
    end
    colorCycleConnections = {}
end

game:GetService("Players").PlayerRemoving:Connect(function(player)
    if player == game:GetService("Players").LocalPlayer then
        cleanupConnections()
    end
end)

connectToPetImages()
