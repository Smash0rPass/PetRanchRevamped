    NumberSequenceKeypoint.new(0, 0),
    NumberSequenceKeypoint.new(0.31, 0.58),
    NumberSequenceKeypoint.new(0.507, 0.757),
    NumberSequenceKeypoint.new(0.756, 0.994),
    NumberSequenceKeypoint.new(1, 1.29)
})
