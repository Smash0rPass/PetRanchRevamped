hoverFrame.Size = UDim2.new(0, 250, 0, 0)
hoverFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
hoverFrame.BackgroundTransparency = 0
hoverFrame.BorderSizePixel = 0
hoverFrame.Visible = false
hoverFrame.ZIndex = 10
hoverFrame.AutomaticSize = Enum.AutomaticSize.Y
hoverFrame.Parent = MainGui

