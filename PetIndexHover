local function getPetNameFromImageId(imageId)
    local idNumber = imageId:match("(%d+)")
    if not idNumber then return nil end
    
    local formattedId = "rbxassetid://" .. idNumber
    
    for petName, info in pairs(petInfo) do
        if info.petIcon == formattedId then
            return petName, info
        end
    end
    
    return nil
end

